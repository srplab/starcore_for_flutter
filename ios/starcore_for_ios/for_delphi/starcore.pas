function CLEString.SetPChar(InData : PChar) : CLEString;
var
    Len : Integer;
    RetPtr : Pointer;
    LPtr : PVS_CHAR;
    FromCode : array[0..10] of VS_CHAR;
begin
    Clear();
    if not assigned(gBS_CoreShellInterface) then
    begin
        //raise Exception.Create('starcore does not init');
        exit;
    end;
    (*--change instring to ansi--*)
    if SizeOf(Char) = 2 then
    begin
      FromCode[0] := 85;  //'U'
      FromCode[1] := 84;  //'T'
      FromCode[2] := 70;  //'F'
      FromCode[3] := 45;  //'-'
      FromCode[4] := 49;  //'1'
      FromCode[5] := 54;  //'6'
      FromCode[6] := 76;  //'L'
      FromCode[7] := 69;  //'E'
      FromCode[8] := 0;
      RetPtr := SRPCS_UnicodeToAnsi(gBS_CoreShellInterface,PVS_CHAR(@FromCode),VS_CP_ACP,Pointer(PChar(InData)),-1,2);
    end;
    if not assigned(RetPtr) then
    begin
        raise Exception.Create('unicode ['+InData+'] to ansi failed');
        exit;
    end;
//    CopyMemory(Result.Value,PChar(InData),Length(InData));
    Len := SRPMM_strlen(RetPtr);
    GetMem(Value, Len+1);
    //Move(RetPtr,Result.Value,Length(InData));
    SRPMM_memcpy(Value,RetPtr,Len);
    SRPCS_FreeBuf(gBS_CoreShellInterface,RetPtr);
    LPtr := Value;
    Inc(LPtr,Len);
    LPtr^ := 0;
    TType := 1;
    Length := Len;
    Result := self;
end;
function CLEString.SetPCharEx(InData : PChar; InLength:Integer) : CLEString;
var
    Len : Integer;
    RetPtr : Pointer;
    LPtr : PVS_CHAR;
    FromCode : array[0..10] of VS_CHAR;
    OutLen : Integer;
begin
    Clear();
    if not assigned(gBS_CoreShellInterface) then
    begin
        //raise Exception.Create('starcore does not init');
        exit;
    end;
    (*--change instring to ansi--*)
    if SizeOf(Char) = 2 then
    begin
      FromCode[0] := 85;  //'U'
      FromCode[1] := 84;  //'T'
      FromCode[2] := 70;  //'F'
      FromCode[3] := 45;  //'-'
      FromCode[4] := 49;  //'1'
      FromCode[5] := 54;  //'6'
      FromCode[6] := 76;  //'L'
      FromCode[7] := 69;  //'E'
      FromCode[8] := 0;
      RetPtr := SRPCS_UnicodeToAnsiEx(gBS_CoreShellInterface,PVS_CHAR(@FromCode),VS_CP_ACP,Pointer(PChar(InData)),InLength * 2,2,@OutLen);
    end;
    if not assigned(RetPtr) then
    begin
        raise Exception.Create('unicode ['+InData+'] to ansi failed');
        exit;
    end;
//    CopyMemory(Result.Value,PChar(InData),Length(InData));
    Len := OutLen; //SRPMM_strlen(RetPtr);
    GetMem(Value, Len+1);
    //Move(RetPtr,Result.Value,Length(InData));
    SRPMM_memcpy(Value,RetPtr,Len);
    SRPCS_FreeBuf(gBS_CoreShellInterface,RetPtr);
    LPtr := Value;
    Inc(LPtr,Len);
    LPtr^ := 0;
    TType := 1;
    Length := Len;
    Result := self;
end;
procedure CLEString.Clear();
begin
    case TType of
    0 : begin Value := nil;  end;
    1 : begin if assigned(Value) then begin FreeMem(Value); end; Value := nil; end;
    end;
end;

function CLEString.ToString() : string;
var
    RetPtr : Pointer;
    FromCode : array[0..9] of VS_CHAR;
    OutLen : VS_INT32;
begin
    if not assigned(gBS_CoreShellInterface) then
    begin
        //raise Exception.Create('starcore does not init');
        exit;
    end;
    if (TType = 0) or (TType = 1) then
    begin
      if not assigned(Value) then
      begin
        Result := '';
        exit;
      end;
      if SizeOf(Char) = 2 then
      begin
        FromCode[0] := 85;  //'U'
        FromCode[1] := 84;  //'T'
        FromCode[2] := 70;  //'F'
        FromCode[3] := 45;  //'-'
        FromCode[4] := 49;  //'1'
        FromCode[5] := 54;  //'6'
        FromCode[6] := 76;  //'L'
        FromCode[7] := 69;  //'E'
        FromCode[8] := 0;
        RetPtr := SRPCS_AnsiToUnicodeEx(gBS_CoreShellInterface,VS_CP_ACP,PVS_CHAR(@FromCode),PVS_INT8(Pointer(Value)),Length, @OutLen);
      end;
    end
    else
    begin
        raise Exception.Create('PBS_CHAR is empty');
        exit;
    end;
    if not assigned(RetPtr) then
    begin
        raise Exception.Create(' ansi to unicode failed');
        exit;
    end;
    Result := strPas( PChar(RetPtr) );
    SRPCS_FreeBuf(gBS_CoreShellInterface,RetPtr);
end;

function CLEString.ToVSChar() : PVS_CHAR;
begin
  Result := Value;
end;

