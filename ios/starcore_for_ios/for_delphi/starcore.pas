  LocaleID: Integer; Flags: Word; var Params; VarResult, ExcepInfo,
  ArgErr: Pointer): HResult;
var
  IDName    : string;
  CallResult : VS_UWORD;
  RetType    : VS_UINT8;
  RetDouble  : VS_DOUBLE;
  RetInt64   : VS_INT64;
  DispParams : TDispParams;
  Arg_Param  : PVarData;
  InputParamNum : Integer;
  Arg_Int    : Integer;
  Arg_IObject : ISRPVariant;
  Arg_VarData : PVarData;

  i,m,n : Integer;
  InputErrorFlag : Boolean;
  ObjectName : string;

  l_para : Pointer;

begin
  Result := S_OK;
  if DispID = 0 then
  begin
    if VarResult <> NULL then
    begin
      PVarData(VarResult)^.vType := varOleStr;
      PVarData(VarResult)^.vString := SysAllocString(PWideChar(ToString()));
    end;
    exit;
  end;
  IDName := FindNameByID(DispID);
  if Length(IDName) = 0 then
  begin
    Result := DISP_E_EXCEPTION;
    exit;
  end;
  ObjectName := TOVS_STRING(PVS_CHAR(SRPI_GetNameEx(SRP_CLEInterface,CLEObject)));
  DispParams := TDispParams(Params);    (*note : input param order is in reverse *)
  InputParamNum := DispParams.cArgs;
  if IDName = 'IsObject' then
    begin
      if VarResult <> NULL then
      begin
        PVarData(VarResult)^.vType := varBoolean;
        PVarData(VarResult)^.VBoolean := IsObject();
      end;
      exit;
    end
  else if IDName = 'IsParaPkg' then
    begin
      if VarResult <> NULL then
      begin
        PVarData(VarResult)^.vType := varBoolean;
        PVarData(VarResult)^.VBoolean := IsParaPkg();
      end;
      exit;
    end
  else if IDName = 'IsBinBuf' then
    begin
      if VarResult <> NULL then
      begin
        PVarData(VarResult)^.vType := varBoolean;
        PVarData(VarResult)^.VBoolean := IsBinBuf();
      end;
      exit;
    end
  else if IDName = 'IsSXml' then
    begin
      if VarResult <> NULL then
      begin
        PVarData(VarResult)^.vType := varBoolean;
        PVarData(VarResult)^.VBoolean := IsSXml();
      end;
      exit;
    end
  else if IDName = 'IsFunctionPara' then
    begin
      if VarResult <> NULL then
      begin
        PVarData(VarResult)^.vType := varBoolean;
        PVarData(VarResult)^.VBoolean := IsFunctionPara();
      end;
      exit;
    end
  else if IDName = 'IsCommInterface' then
    begin
      if VarResult <> NULL then
      begin
        PVarData(VarResult)^.vType := varBoolean;
        PVarData(VarResult)^.VBoolean := IsCommInterface();
      end;
      exit;
    end
  else if IDName = 'ID' then
    begin
      if ObjectType <> CLEVAR_OBJECT then
      begin
        PExcepInfo(ExcepInfo).bstrDescription := 'variable is not cle object, no ID property';
        Result := DISP_E_EXCEPTION;
        exit;
      end;
      if VarResult <> NULL then
      begin
        PVarData(VarResult)^.vType := varOleStr;
        PVarData(VarResult)^.vString := SysAllocString(PWideChar(TOVS_STRING(SRPI_UuidToString(SRP_CLEInterface,SRPI_GetIDEx(SRP_CLEInterface,CLEObject)))));
      end;
      exit;
    end
  else if IDName = 'ToString' then
    begin
      if VarResult <> NULL then
      begin
        PVarData(VarResult)^.vType := varOleStr;
        PVarData(VarResult)^.vString := SysAllocString(PWideChar(ToString()));
      end;
      exit;
    end;
    
  if ObjectType <> CLEVAR_OBJECT then
  begin
    PExcepInfo(ExcepInfo).bstrDescription := 'Only cle object can be called with Variant';
    Result := DISP_E_EXCEPTION;
    exit;
  end;    

  case Flags of
  DISPATCH_METHOD,$3      :
     begin
         (* get function name based on DispID *)
         (*----function----*)
         if IDName = 'Get' then
         begin
           if (InputParamNum = 1) and (ParamIsInteger(@DispParams.rgvarg[0])) then
           begin
             (*--get value by index--*)
             Arg_Int := ParamToInteger(@DispParams.rgvarg[0]);
             CallResult := SRPI_ScriptGetObjectIndexEx(SRP_CLEInterface,CLEObject,Arg_Int,@RetType,@RetDouble,@RetInt64);
             if RetCallResult(VarResult,CallResult,RetType,RetDouble,RetInt64) = False then
             begin
               PExcepInfo(ExcepInfo).bstrDescription := 'can not get object [' + ObjectName + '] property at ' + IntToStr(Arg_Int) + ' . Index maybe out of range';
               Result := DISP_E_EXCEPTION;
             end;
             exit;
           end;
         end;
         if IDName = 'Set' then
         begin
         if (InputParamNum = 2) and (ParamIsInteger(@DispParams.rgvarg[1])) then
           begin
             (*--get value by index--*)
             Arg_Int := ParamToInteger(@DispParams.rgvarg[1]);
             Arg_Param := PVarData(@DispParams.rgvarg[0]);
             case (varTypeMask and DispParams.rgvarg[0].vt) of
             varBoolean:
             begin
               SRPI_ScriptSetBoolIndex(SRP_CLEInterface,CLEObject,Arg_Int,Arg_Param.VBoolean);
             end;
             varInteger:
             begin
               SRPI_ScriptSetIntIndex(SRP_CLEInterface,CLEObject,Arg_Int,Arg_Param.VInteger);
             end;
             varInt64,varUInt64:
             begin
               SRPI_ScriptSetInt64Index(SRP_CLEInterface,CLEObject,Arg_Int,Arg_Param.VInt64);
             end;
             varSingle:
             begin
               SRPI_ScriptSetNumberIndex(SRP_CLEInterface,CLEObject,Arg_Int,Arg_Param.VSingle);
             end;
             varDouble:
             begin
               SRPI_ScriptSetDoubleIndex(SRP_CLEInterface,CLEObject,Arg_Int,Arg_Param.VDouble);
             end;
             varOleStr:
             begin
               SRPI_ScriptSetStrIndex(SRP_CLEInterface,CLEObject,Arg_Int,TOVS_CHAR(OleStrToString(Arg_Param.VOleStr)));
             end;
             varVariant,varDispatch:
             begin
               if (varTypeMask and Arg_Param.VType) = varVariant then
               begin
                 Arg_VarData := PVarData(Arg_Param.VPointer);
                 Arg_IObject := ISRPVariant(Arg_VarData.VPointer);                 
               end
               else
                 Arg_IObject := ISRPVariant(Arg_Param.VPointer);
               if Arg_IObject.IsObject() then
                 SRPI_ScriptSetObjectIndex(SRP_CLEInterface,CLEObject,Arg_Int,VSTYPE_OBJPTR,VS_UWORD(Arg_IObject.ToObject()))
               else if Arg_IObject.IsParaPkg() then
                 SRPI_ScriptSetObjectIndex(SRP_CLEInterface,CLEObject,Arg_Int,VSTYPE_PARAPKGPTR,VS_UWORD(Arg_IObject.ToObject()))
               else if Arg_IObject.IsBinBuf() then
                 SRPI_ScriptSetObjectIndex(SRP_CLEInterface,CLEObject,Arg_Int,VSTYPE_BINBUFPTR,VS_UWORD(Arg_IObject.ToObject()))
               else
                 Result := DISP_E_EXCEPTION
             end;
             else
               Result := DISP_E_EXCEPTION;
             end;
             exit;
           end;
         end;
         (* first we get property *)
         if InputParamNum = 0 then
         begin
           (*----is property?----*)
           CallResult := SRPI_ScriptGetObjectEx(SRP_CLEInterface,CLEObject,TOVS_CHAR(IDName),@RetType,@RetDouble,@RetInt64);
           if RetType <> VSTYPE_IGNORE then
           begin
             if RetCallResult(VarResult,CallResult,RetType,RetDouble,RetInt64) <> False then
               exit; (*--return--*)
           end;
           (*--call function--*)
         end;
         (* function process : function name is IDName*)
         (* push parameter to lua stack *)
         n := SRPI_LuaGetTop(SRP_CLEInterface);
         (* If no input parameter, then the input arg number is 1 and type is varError *)
         if InputParamNum = 1 then
         begin
           Arg_Param := PVarData(@DispParams.rgvarg[0]);
           if Arg_Param.VType = varError then
             InputParamNum := 0;
         end;

         if IDName = 'create' then
         begin
           (*--create instance --*)
           if InputParamNum = 0 then
           begin
             CallResult := VS_UWORD(SRPI_MallocObjectL(SRP_CLEInterface,SRPI_GetIDEx(SRP_CLEInterface,CLEObject),0,NULL));
             RetCallResult(VarResult,CallResult,VSTYPE_OBJPTR,0,0);
             exit;
           end;
           l_para := SRPI_GetParaPkgInterface(SRP_CLEInterface);
           InputErrorFlag := false;
           for i := 0 to InputParamNum - 1 do
           begin
             Arg_Param := PVarData(@DispParams.rgvarg[InputParamNum-1-i]);
             case (varTypeMask and Arg_Param.VType) of
               varBoolean: SRPParaPkg_InsertBool(l_para,i,Arg_Param.VBoolean);
               varSmallint,varWord:SRPParaPkg_InsertInt(l_para,i,Arg_Param.VSmallInt);
               varInteger,varLongWord: SRPParaPkg_InsertInt(l_para,i,Arg_Param.VInteger);
               varInt64,varUInt64: SRPParaPkg_InsertInt64(l_para,i,Arg_Param.VInt64);
               varSingle: SRPParaPkg_InsertFloat(l_para,i,Arg_Param.VSingle);
               varDouble: SRPParaPkg_InsertFloat(l_para,i,Arg_Param.VDouble);
               varOleStr: SRPParaPkg_InsertStr(l_para,i,TOVS_CHAR(OleStrToString(Arg_Param.VOleStr)));
               varVariant,varDispatch:
                 begin
                   if (varTypeMask and Arg_Param.VType) = varVariant then
                   begin
                     Arg_VarData := PVarData(Arg_Param.VPointer);
                     Arg_IObject := ISRPVariant(Arg_VarData.VPointer);                 
                   end
                   else
                     Arg_IObject := ISRPVariant(Arg_Param.VPointer);
                   if Arg_IObject.IsObject() then
                     SRPParaPkg_InsertObject(l_para,i,Arg_IObject.ToObject())
                   else if Arg_IObject.IsParaPkg() then
                     SRPParaPkg_InsertParaPackage(l_para,i,Arg_IObject.ToObject())
                   else if Arg_IObject.IsBinBuf() then
                     SRPParaPkg_InsertBin(l_para,i,SRPBinBuf_GetBufPtr(Arg_IObject.ToObject(),0),SRPBinBuf_GetSize(Arg_IObject.ToObject()))
                   else
                   begin
                     Result := DISP_E_EXCEPTION;
                     InputErrorFlag := true;
                   end;
                 end;
               else
                 begin
                   Result := DISP_E_EXCEPTION;
                   InputErrorFlag := true;
                 end;
             end;
           end;
           if InputErrorFlag = true then
             exit
           else
           begin
             CallResult := VS_UWORD(SRPI_IMallocObjectL(SRP_CLEInterface,SRPI_GetIDEx(SRP_CLEInterface,CLEObject),l_para));
             SRPParaPkg_Release(l_para);
             RetCallResult(VarResult,CallResult,VSTYPE_OBJPTR,0,0);
             exit;
           end;
         end;

         InputErrorFlag := false;
         for i := 0 to InputParamNum - 1 do
         begin
           Arg_Param := PVarData(@DispParams.rgvarg[InputParamNum-1-i]);
           case (varTypeMask and Arg_Param.VType) of
             varBoolean: SRPI_LuaPushBool(SRP_CLEInterface,Arg_Param.VBoolean);
             varSmallint,varWord:SRPI_LuaPushInt(SRP_CLEInterface,Arg_Param.VSmallInt);
             varInteger,varLongWord: SRPI_LuaPushInt(SRP_CLEInterface,Arg_Param.VInteger);
             varInt64,varUInt64: SRPI_LuaPushInt64(SRP_CLEInterface,Arg_Param.VInt64);
             varSingle: SRPI_LuaPushNumber(SRP_CLEInterface,Arg_Param.VSingle);
             varDouble: SRPI_LuaPushNumber(SRP_CLEInterface,Arg_Param.VDouble);
             varOleStr: SRPI_LuaPushString(SRP_CLEInterface,TOVS_CHAR(OleStrToString(Arg_Param.VOleStr)));
             varVariant,varDispatch:
               begin
                 if (varTypeMask and Arg_Param.VType) = varVariant then
                 begin
                   Arg_VarData := PVarData(Arg_Param.VPointer);
                   Arg_IObject := ISRPVariant(Arg_VarData.VPointer);                 
                 end
                 else
                   Arg_IObject := ISRPVariant(Arg_Param.VPointer);
                 if Arg_IObject.IsObject() then
                   SRPI_LuaPushObject(SRP_CLEInterface,Arg_IObject.ToObject())
                 else if Arg_IObject.IsParaPkg() then
                   SRPI_LuaPushParaPkg(SRP_CLEInterface,Arg_IObject.ToObject(),VS_FALSE)
                 else if Arg_IObject.IsBinBuf() then
                   SRPI_LuaPushBinBuf(SRP_CLEInterface,Arg_IObject.ToObject(),VS_FALSE)
                 else
                 begin
                   Result := DISP_E_EXCEPTION;
                   InputErrorFlag := true;
                 end;
               end;
             else
               begin
                 Result := DISP_E_EXCEPTION;
                 InputErrorFlag := true;
               end;
           end;
         end;
         if InputErrorFlag = true then
           exit
         else
         begin
           (*--call function --*)
           if SRPI_LuaCall( SRP_CLEInterface,CLEObject, IDName, InputParamNum,-1) = VS_FALSE then
           begin
             PExcepInfo(ExcepInfo).bstrDescription := 'Call object [' + ObjectName + '] function ' + IDName + ' failed';
             m := SRPI_LuaGetTop(SRP_CLEInterface);
		         if( m > n ) then
               SRPI_LuaPop(SRP_CLEInterface,m-n);
             Result := DISP_E_EXCEPTION;
             exit;
           end
           else
           begin
             (*-- process result --*)
             m := SRPI_LuaGetTop(SRP_CLEInterface);
             if m-n = 0 then
               exit;  (* no result *)
             if RetLuaResult(VarResult,m-n) = False then
             begin
               Result := DISP_E_EXCEPTION;
               PExcepInfo(ExcepInfo).bstrDescription := 'Call object [' + ObjectName + '] function ' + IDName + ' failed. Result can not be converted to delphi';
             end;
             if( m > n ) then
               SRPI_LuaPop(SRP_CLEInterface,m-n);
           end;
         end;
     end;
  DISPATCH_PROPERTYGET :
     begin
         (* get function name based on DispID *)
         CallResult := SRPI_ScriptGetObjectEx(SRP_CLEInterface,CLEObject,TOVS_CHAR(IDName),@RetType,@RetDouble,@RetInt64);
         if RetCallResult(VarResult,CallResult,RetType,RetDouble,RetInt64) = False then
           Result := DISP_E_EXCEPTION;
     end;
  DISPATCH_PROPERTYPUT :
     begin
         (* put function name based on DispID *)
         if InputParamNum = 1  then
         begin
             Arg_Param := PVarData(@DispParams.rgvarg[0]);
             case (varTypeMask and DispParams.rgvarg[0].vt) of
             varBoolean:
             begin
               SRPI_ScriptSetBool(SRP_CLEInterface,CLEObject,IDName,Arg_Param.VBoolean);
             end;
             varInteger:
             begin
               SRPI_ScriptSetInt(SRP_CLEInterface,CLEObject,IDName,Arg_Param.VInteger);
             end;
             varInt64,varUInt64:
             begin
               SRPI_ScriptSetInt64(SRP_CLEInterface,CLEObject,IDName,Arg_Param.VInt64);
             end;
             varSingle:
             begin
               SRPI_ScriptSetNumber(SRP_CLEInterface,CLEObject,IDName,Arg_Param.VSingle);
             end;
             varDouble:
             begin
               SRPI_ScriptSetDouble(SRP_CLEInterface,CLEObject,IDName,Arg_Param.VDouble);
             end;
             varOleStr:
             begin
               SRPI_ScriptSetStr(SRP_CLEInterface,CLEObject,IDName,TOVS_CHAR(OleStrToString(Arg_Param.VOleStr)));
             end;
             varVariant,varDispatch:
             begin
               if (varTypeMask and Arg_Param.VType) = varVariant then
               begin
                 Arg_VarData := PVarData(Arg_Param.VPointer);
                 Arg_IObject := ISRPVariant(Arg_VarData.VPointer);                 
               end
               else
                 Arg_IObject := ISRPVariant(Arg_Param.VPointer);
               if Arg_IObject.IsObject() then
                 SRPI_ScriptSetObject(SRP_CLEInterface,CLEObject,IDName,VSTYPE_OBJPTR,VS_UWORD(Arg_IObject.ToObject()))
               else if Arg_IObject.IsParaPkg() then
                 SRPI_ScriptSetObject(SRP_CLEInterface,CLEObject,IDName,VSTYPE_PARAPKGPTR,VS_UWORD(Arg_IObject.ToObject()))
               else if Arg_IObject.IsCommInterface() then
                 SRPI_ScriptSetObject(SRP_CLEInterface,CLEObject,IDName,VSTYPE_BINBUFPTR,VS_UWORD(Arg_IObject.ToObject()))
               else
                 Result := DISP_E_EXCEPTION
             end;
             else
               Result := DISP_E_EXCEPTION;
             end;
             exit;
         end
         else
           Result := DISP_E_EXCEPTION;
     end;
  else
     Result := DISP_E_EXCEPTION;
  end;
end;

function SRPOBJECT_TOVARIANT(X:Pointer;NeedRelease:Boolean):variant;
var
  tv : TSRPVariant;
begin
  tv := TSRPVariant.Create(x,CLEVAR_OBJECT,NeedRelease);
  Result := IDispatch(tv);
  tv._Release;
end;
function SRPPARAPKG_TOVARIANT(X:Pointer;NeedRelease:Boolean):variant;
var
  tv : TSRPVariant;
  tp : TSRPParaPkg;
begin
  try
    tp := TSRPParaPkg(x);
    tv := TSRPVariant.Create(tp.CLEObject,CLEVAR_PARAPKG,NeedRelease);
    Result := variant(IDispatch(tv));
    tv._Release;
  except
    tv := TSRPVariant.Create(x,CLEVAR_PARAPKG,NeedRelease);
    Result := IDispatch(tv);
    tv._Release;
  end;
end;
