  jvs[2].z := 0;
  jvs[3].z := 1;
  jvs[4].l := StringToJNIString(JavaEnv,'');
  jvs[5].i := PortNumberForDebug;
  jvs[6].l := StringToJNIString(JavaEnv,'');
  jvs[7].i := PortNumberForDirectClient;
  IntTemp := JavaEnv^.CallIntMethodA(JavaEnv,starcoreobj,jMethodID,@jvs);

  if IntTemp = -1 then
  begin
     Result := -1;
     exit;
  end;
  if _StarCore_Init() = false then
  begin
      Result := -1;
  end
  else
  begin
      Result := IntTemp;
      ControlInterface := VSCore_QueryControlInterface();
      gBS_CoreShellInterface := SRPControl_GetCoreShellInterface( ControlInterface);
      SRPControl_Release(ControlInterface);
  end;
end;

function StarCore_InjectClass(ClassName:string) : VS_BOOL;
var
  jMethodID:JNIMethodID;
  jLoadedClass:JNIObject;
  jvs: array[0..1] of JNIValue;

begin
{$IFDEF USE_DEXFILE}
  Result := false;
{$ELSE}
  jLoadedClass:= TJNIResolver.ClassLoader.LoadClass(ClassName);
  if not assigned(jLoadedClass) then
  begin
    Result := false;
    exit;
  end;
  jMethodID := TJNIResolver.GetJavaMethodID(jFactoryClass,'_InjectClass','(Ljava/lang/String;Ljava/lang/Class;)V');
  jvs[0].l := StringToJNIString(JavaEnv,ClassName);
  jvs[1].l := jLoadedClass;
{$ENDIF}
  JavaEnv^.CallVoidMethodA(JavaEnv,starcoreobj,jMethodID,@jvs);
  Result := true;
end;

procedure VSCore_Term( );
var
  jMethodID:JNIMethodID;

begin
{$IFDEF USE_DEXFILE}
  jMethodID := TJNIResolver.GetJavaMethodID((jFactoryClass as ILocalObject).GetObjectID,'_ModuleExit','()V');
{$ELSE}
  jMethodID := TJNIResolver.GetJavaMethodID(jFactoryClass,'_ModuleExit','()V');
{$ENDIF}
  JavaEnv^.CallIntMethod(JavaEnv,starcoreobj,jMethodID);
end;

procedure VSCore_TermEx( );
var
  jMethodID:JNIMethodID;

begin
{$IFDEF USE_DEXFILE}
  jMethodID := TJNIResolver.GetJavaMethodID((jFactoryClass as ILocalObject).GetObjectID,'_ModuleExit','()V');
{$ELSE}
  jMethodID := TJNIResolver.GetJavaMethodID(jFactoryClass,'_ModuleExit','()V');
{$ENDIF}
  JavaEnv^.CallIntMethod(JavaEnv,starcoreobj,jMethodID);
end;

{$ENDIF}
