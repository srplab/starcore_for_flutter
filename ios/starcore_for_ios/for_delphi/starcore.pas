 function SRPVARIANT_TOPOINTER(X:variant):Pointer;
 var
   Arg_Param,Arg_VarData : PVarData;
   Arg_IObject : ISRPVariant;
 begin
   Arg_Param := PVarData(@X);
   if (varTypeMask and Arg_Param.VType) = varVariant then
   begin
     Arg_VarData := PVarData(Arg_Param.VPointer);
     Arg_IObject := ISRPVariant(Arg_VarData.VPointer);
   end
   else
     Arg_IObject := ISRPVariant(Arg_Param.VPointer);
   Result := Arg_IObject.ToObject();; (*ISRPVariant(IDispatch(x)).ToObject();*)
 end;
