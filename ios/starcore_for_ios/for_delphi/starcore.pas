class operator CLEString.Implicit(InData : string) : CLEString;
var
    Len : Integer;
    OutLen : Integer;
    RetPtr : Pointer;
    LPtr : PVS_CHAR;
    FromCode : array[0..10] of VS_CHAR;
begin
    if not assigned(gBS_CoreShellInterface) then
    begin
        //raise Exception.Create('starcore does not init');
        exit;
    end;
    (*--change instring to ansi--*)
    if SizeOf(Char) = 2 then
    begin
      FromCode[0] := 85;  //'U'
      FromCode[1] := 84;  //'T'
      FromCode[2] := 70;  //'F'
      FromCode[3] := 45;  //'-'
      FromCode[4] := 49;  //'1'
      FromCode[5] := 54;  //'6'
      FromCode[6] := 76;  //'L'
      FromCode[7] := 69;  //'E'
      FromCode[8] := 0;
      RetPtr := SRPCS_UnicodeToAnsiEx(gBS_CoreShellInterface,PVS_CHAR(@FromCode),VS_CP_ACP,Pointer(PChar(InData)),System.Length(InData) * 2,2,@OutLen);
    end;
    if not assigned(RetPtr) then
    begin
        raise Exception.Create('unicode ['+InData+'] to ansi failed');
        exit;
    end;
//    CopyMemory(Result.Value,PChar(InData),Length(InData));
    Len := OutLen; //SRPMM_strlen(RetPtr);
    GetMem(Result.Value, Len+1);
    //Move(RetPtr,Result.Value,Length(InData));
    SRPMM_memcpy(Result.Value,RetPtr,Len);
    SRPCS_FreeBuf(gBS_CoreShellInterface,RetPtr);
    LPtr := Result.Value;
    Inc(LPtr,Len);
    LPtr^ := 0;
    Result.TType := 1;
    Result.Length := OutLen;
end;

