  tp : PFuncOrVarNameNode;
begin
  Result := '';
  tp:= NameNodeRoot;
  while tp <> NULL do
  begin
   if tp.Value = ID then
   begin
     Result := tp.Name
   end;
   tp := tp.Next
  end;
end;

function TSRPVariant.GetIDsOfNames(const IID: TGUID; Names: Pointer;
  NameCount, LocaleID: Integer; DispIDs: Pointer): HResult;
var
  oslNames : POLEStrList;
  IDName    : string;
  IDValue   : VS_UINT32;
  VSStr     : PVS_CHAR;
  pdidDispIDs : PDispIDList;
  Node      : PFuncOrVarNameNode;
begin
  (*
  if (ObjectType <> CLEVAR_OBJECT) and (ObjectType <> CLEVAR_PARAPKG) and (ObjectType <> CLEVAR_BINBUF) then
  begin
    Result := DISP_E_UNKNOWNNAME;
    exit;
  end;
  *)
  Result := S_OK;
  oslNames := Names;
  IDName := oslNames[0];
  VSStr := TOVS_CHAR(IDName);
  pdidDispIDs := DispIDs;
  IDValue := SRPI_GetHashValue(SRP_CLEInterface,VSStr,SRPMM_strlen(VSStr),0);
  if Length(FindNameByID(IDValue)) <> 0 then
  begin
      pdidDispIDs[0] := IDValue;
      exit;
  end
  else
  begin
      (*--create new index --*)
      Node := New(PFuncOrVarNameNode);
      Node.Value := IDValue;
      Node.Name := IDName;
      Node.Forw := NULL;
      Node.Next := NULL;
      if NameNodeRoot = NULL then
        NameNodeRoot := Node
      else
      begin
        NameNodeRoot.Forw := Node;
        Node.Next := NameNodeRoot;
        NameNodeRoot := Node;
      end;
      pdidDispIDs[0] := IDValue;
  end;
end;

