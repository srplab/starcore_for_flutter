function VS_ISMODULEEXIST_PROC(CLEType:VS_UINT16;X: VS_UINT16): VS_BOOL;
begin
    Result := ( (X = 0) or
              ( ((CLEType = VS_SERVER) or (CLEType = VS_SERVER_SERVER)) and ((X and VSMODULE_SERVER_SERVER) <> 0) ) or
              ( ((CLEType = VS_CLIENT) or (CLEType = VS_CLIENT_USER)) and (( X and VSMODULE_CLIENT_USER) <> 0 ) ) or
              ( (CLEType = VS_DEBUG)  and (( X and VSMODULE_DEBUG )<> 0 ) ) or
