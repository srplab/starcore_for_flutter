              ( (CLEType = VS_CLIENT_COOPERATOR) and ((X and VSMODULE_CLIENT_COOPERATOR) <> 0 )) or
              ( (CLEType = VS_CLIENT_CALLER) and ((X and VSMODULE_CLIENT_CALLER) <> 0 )) );

end;

