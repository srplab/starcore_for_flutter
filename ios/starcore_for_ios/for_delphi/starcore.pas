function TSRPVariant.ParamIsInteger(Param:PVarData) : Boolean; stdcall;
begin
 if (Param.vType = varSmallint) or (Param.vType = varInteger) or (Param.vType = varShortInt) or (Param.vType = varByte) or (Param.vType = varWord) or (Param.vType = varLongWord) or (Param.vType = varInt64) or (Param.vType = varUInt64) then
   Result := true
 else
   Result := false
end;

function TSRPVariant.ParamToInteger(Param:PVarData) : Integer; stdcall;
begin
  case Param.vType of
  varSmallint : Result := Integer(Param.VSmallInt);
  varShortInt : Result := Integer(Param.VShortInt);
  varInteger  : Result := Integer(Param.VInteger);
  varByte     : Result := Integer(Param.VByte);
  varWord     : Result := Integer(Param.VWord);
  varLongWord : Result := Integer(Param.VLongWord);
  varInt64 : Result := Integer(Param.VInt64);
  varUInt64 : Result := Integer(Param.VUInt64);
  else
    Result := 0;
  end;
