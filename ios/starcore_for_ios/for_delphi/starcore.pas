function StarCore_Init(PackageName:string;DexFileName:string) : VS_BOOL;
var
  context:JContext;
  optimizedpath_jfile:JFile;
  dexpath_jstring,optimizedpath_jstring:JString;
  cl:JDexClassLoader;
  jLoadedClass:Jlang_Class;
  jFiledID:JNIFieldID;
  jMethodID:JNIMethodID;

  IntTemp : VS_INT64;

begin
  context:=SharedActivityContext;
  JavaEnv:=TJNIResolver.GetJNIEnv;

  dexpath_jstring:=StringToJString('/data/data/' + PackageName + '/files/'+DexFileName);
  optimizedpath_jfile:=context.getDir(StringToJString('outdex'),TJContext.javaclass.mode_private);
  optimizedpath_jstring:=optimizedpath_jfile.getAbsolutePath;

  cl:=TJDexClassLoader.JavaClass.init(dexpath_jstring, optimizedpath_jstring,nil, TJDexClassLoader.JavaClass.getSystemClassLoader);

  if not assigned(cl) then
  begin
    Result := false;
    exit;
  end;

  jLoadedClass:=cl.loadclass(stringtoJString('com/srplab/www/starcore/StarCoreFactoryPath'));
  jFactoryClass := cl.loadclass(stringtoJString('com/srplab/www/starcore/StarCoreFactory'));
  if not assigned(jFactoryClass) then
  begin
    Result := false;
    exit;
  end;

  jFiledID := TJNIResolver.GetJavaStaticFieldID((jLoadedClass as ILocalObject).GetObjectID,'StarCoreCoreLibraryPath','Ljava/lang/String;');
  JavaEnv^.SetStaticObjectField(JavaEnv,(jLoadedClass as ILocalObject).GetObjectID,jFiledID,StringToJNIString(JavaEnv,'/data/data/'+PackageName+'/lib')); //StringToJString('/data/data/com.embarcadero.Project1/lib'));

  jFiledID := TJNIResolver.GetJavaStaticFieldID((jLoadedClass as ILocalObject).GetObjectID,'StarCoreShareLibraryPath','Ljava/lang/String;');
  JavaEnv^.SetStaticObjectField(JavaEnv,(jLoadedClass as ILocalObject).GetObjectID,jFiledID,StringToJNIString(JavaEnv,'/data/data/'+PackageName+'/lib'));

  jFiledID := TJNIResolver.GetJavaStaticFieldID((jLoadedClass as ILocalObject).GetObjectID,'ActivityObject','Ljava/lang/Object;');
  JavaEnv^.SetStaticObjectField(JavaEnv,(jLoadedClass as ILocalObject).GetObjectID,jFiledID,(SharedActivity as ILocalObject).GetObjectID);

  //---call java init function
  jMethodID := TJNIResolver.GetJavaStaticMethodID((jFactoryClass as ILocalObject).GetObjectID,'GetFactory','()Lcom/srplab/www/starcore/StarCoreFactory;');
  starcoreobj := JavaEnv^.CallStaticObjectMethod(JavaEnv,(jFactoryClass as ILocalObject).GetObjectID,jMethodID);
  if not assigned(starcoreobj) then
  begin
    Result := false;
    exit;
  end;

  jMethodID := TJNIResolver.GetJavaMethodID((jFactoryClass as ILocalObject).GetObjectID,'_CoreHandle','()J');
  IntTemp:= JavaEnv^.CallLongMethod(JavaEnv,starcoreobj,jMethodID);
  if IntTemp = 0 then
  begin
     Result := false;
     exit;
  end;
  hStarCoreDllHandle := THandle(IntTemp);
  Result := true;
end;
