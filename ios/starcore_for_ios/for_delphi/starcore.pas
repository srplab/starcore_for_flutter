function TSRPVariant.RetCallResult(var VarResult;CallResult:VS_UWORD;RetType:VS_UINT8;RetDouble:VS_DOUBLE;RetInt64:VS_INT64):Boolean; stdcall;
var
  RetObject  : Pointer;
  RetString  : string;
begin
  Result := True;
  case RetType of
  VSTYPE_BOOL :
    begin
      PVarData(VarResult)^.vType := varBoolean;
      PVarData(VarResult)^.VBoolean := Boolean(CallResult);
    end;
  VSTYPE_INT32 :
    begin
      PVarData(VarResult)^.vType := varInteger;
      PVarData(VarResult)^.vInteger := Integer(CallResult);
    end;
  VSTYPE_INT64 :
    begin
      PVarData(VarResult)^.vType := varInt64;
      PVarData(VarResult)^.VInt64 := VS_INT64(CallResult);
    end;
  VSTYPE_UWORD :
    begin
{$IF SIZEOF(Pointer) = 8}
      PVarData(VarResult)^.vType := varInt64;
      PVarData(VarResult)^.VInt64 := VS_INT64(CallResult);
{$ELSE}
      PVarData(VarResult)^.vType := varInteger;
      PVarData(VarResult)^.VInt64 := Integer(CallResult);
{$IFEND}
    end;
  VSTYPE_DOUBLE :
    begin
      PVarData(VarResult)^.vType := varDouble;
      PVarData(VarResult)^.vDouble := RetDouble;
    end;
  VSTYPE_CHARPTR,VSTYPE_VSTRING : (* VSTYPE_VSTRING is bug before 2.5.2,  is same as VSTYPE_CHARPTR *)
    begin
      RetString := TOVS_STRING(PVS_CHAR(CallResult));
      PVarData(VarResult)^.vType := varOleStr;
      PVarData(VarResult)^.vString := SysAllocString(PWideChar(RetString));
    end;
  VSTYPE_OBJPTR :
    begin
      PVarData(VarResult)^.vType := varDispatch;
      RetObject := Pointer(CallResult);
      PVarData(VarResult)^.VDispatch := Pointer(IDispatch(TSRPVariant.Create(RetObject,false)));
    end;
  VSTYPE_PARAPKGPTR :
    begin
      PVarData(VarResult)^.vType := varDispatch;
      RetObject := Pointer(CallResult);
      PVarData(VarResult)^.VDispatch := Pointer(IDispatch(TSRPVariant.Create(RetObject,CLEVAR_PARAPKG,false)));
    end;
  VSTYPE_BINBUFPTR :
    begin
      PVarData(VarResult)^.vType := varDispatch;
      RetObject := Pointer(CallResult);
      PVarData(VarResult)^.VDispatch := Pointer(IDispatch(TSRPVariant.Create(RetObject,CLEVAR_BINBUF,false)));
    end;
  else
    Result := False;
  end;
end;

function TSRPVariant.LuaTableToDelphi(Index:Integer):variant;stdcall;
var
  Size : Integer;
  IntTemp : Integer;
  ResultArray: variant;
  RetResult_Bool : Boolean;
  RetResult_VarData : variant;
begin
  Size := 0;
	SRPI_LuaPushNil(SRP_CLEInterface);
  while( SRPI_LuaNext( SRP_CLEInterface, Index ) <> 0 ) do
  begin
    case SRPI_LuaType(SRP_CLEInterface,-2) of
    VSLUATYPE_INT,VSLUATYPE_INT64,VSLUATYPE_UWORD,VSLUATYPE_NUMBER :
      begin
        IntTemp := SRPI_LuaToInt(SRP_CLEInterface,-2);
        if( IntTemp > 0 ) then
          if( IntTemp > Size ) then
            Size := IntTemp
        else
          begin
            TVarData(Result).VType := varEmpty;
            SRPI_LuaPop(SRP_CLEInterface,2);
            exit;
          end;
      end
      else
      begin
        TVarData(Result).VType := varEmpty;
        SRPI_LuaPop(SRP_CLEInterface,2);
        exit;
      end;
    end;
    SRPI_LuaPop(SRP_CLEInterface,1);
  end;
  (*----------------------*)
  ResultArray := VarArrayCreate([1,Size], varVariant);
  SRPI_LuaPushNil(SRP_CLEInterface);
  while( SRPI_LuaNext( SRP_CLEInterface, Index ) <> 0 ) do
  begin
    case SRPI_LuaType(SRP_CLEInterface,-2) of
    VSLUATYPE_INT,VSLUATYPE_INT64,VSLUATYPE_UWORD,VSLUATYPE_NUMBER :
      begin
        IntTemp := SRPI_LuaToInt(SRP_CLEInterface,-2);
        if (IntTemp > 0) and (IntTemp <= Size) then
        begin
          RetResult_VarData := GetLuaValueAtIndex(  -1, RetResult_Bool );
          if RetResult_Bool = false then
          begin
            TVarData(Result).VType := varEmpty;
            SRPI_LuaPop(SRP_CLEInterface,2);
            exit;
          end;
          ResultArray[IntTemp] := RetResult_VarData;
        end
        else
          begin
            TVarData(Result).VType := varEmpty;
            SRPI_LuaPop(SRP_CLEInterface,2);
            exit;
          end;
      end
      else
      begin
        TVarData(Result).VType := varEmpty;
        SRPI_LuaPop(SRP_CLEInterface,2);
        exit;
      end;
    end;
    SRPI_LuaPop(SRP_CLEInterface,1);
  end;
  Result := ResultArray;
end;

function TSRPVariant.GetLuaValueAtIndex(Index:Integer;out RetResult:Boolean):variant;stdcall;
var
  RetString : string;
  RetObject  : Pointer;
begin
  RetResult := true;
  case SRPI_LuaType(SRP_CLEInterface,Index) of
  VSLUATYPE_INT :
    begin
      TVarData(Result).vType := varInteger;
      TVarData(Result).vInteger := Integer(SRPI_LuaToInt(SRP_CLEInterface,Index));
    end;
  VSLUATYPE_INT64 :
    begin
      TVarData(Result).vType := varInt64;
      TVarData(Result).VInt64 := VS_INT64(SRPI_LuaToInt64(SRP_CLEInterface,Index));
    end;
  VSLUATYPE_UWORD :
    begin
{$IF SIZEOF(Pointer) = 8 }
      TVarData(Result).vType := varInt64;
      TVarData(Result).VInt64 := VS_INT64(SRPI_LuaToUWord(SRP_CLEInterface,Index));
{$ELSE}
      TVarData(Result).vType := varInteger;
      TVarData(Result).vInteger := Integer(SRPI_LuaToUWord(SRP_CLEInterface,Index));
{$IFEND}
    end;
  VSLUATYPE_NUMBER :
    begin
      TVarData(Result).vType := varDouble;
      TVarData(Result).vDouble := VS_DOUBLE(SRPI_LuaToNumber(SRP_CLEInterface,Index));
    end;
  VSLUATYPE_BOOL :
    begin
      TVarData(Result).vType := varBoolean;
      TVarData(Result).VBoolean := Boolean(SRPI_LuaToBool(SRP_CLEInterface,Index));
    end;
  VSLUATYPE_STRING :
    begin
      if SRPI_LuaToString(SRP_CLEInterface,Index) = NULL  then
      begin
        TVarData(Result).vType := varNull;
        TVarData(Result).vPointer := NULL;
      end
      else
      begin
        RetString := TOVS_STRING(PVS_CHAR(SRPI_LuaToString(SRP_CLEInterface,Index)));
        TVarData(Result).vType := varOleStr;
        TVarData(Result).vString := SysAllocString(PWideChar(RetString));
      end;
    end;
  VSLUATYPE_TABLE :
    begin
			if( SRPI_LuaTableToParaPkg(SRP_CLEInterface,Index,NULL,VS_TRUE) = VS_TRUE ) then
      begin
				Result := LuaTableToDelphi(Index );
        exit;
      end;
        TVarData(Result).vType := varDispatch;
        RetObject := Pointer(SRPI_LuaToRaw(SRP_CLEInterface,Index,VS_FALSE));
        TVarData(Result).VDispatch := Pointer(IDispatch(TSRPVariant.Create(RetObject,true)));
    end;
  VSLUATYPE_FUNCTION,VSLUATYPE_USERDATA,VSLUATYPE_LIGHTUSERDATA :
    begin
        TVarData(Result).vType := varDispatch;
        RetObject := Pointer(SRPI_LuaToRaw(SRP_CLEInterface,Index,VS_FALSE));
        TVarData(Result).VDispatch := Pointer(IDispatch(TSRPVariant.Create(RetObject,true)));
    end;
  VSLUATYPE_OBJECT :
    begin
        TVarData(Result).vType := varDispatch;
        RetObject := Pointer(SRPI_LuaToObject(SRP_CLEInterface,Index));
        TVarData(Result).VDispatch := Pointer(IDispatch(TSRPVariant.Create(RetObject,false)));
    end;
  VSLUATYPE_PARAPKG :
    begin
        TVarData(Result).vType := varDispatch;
        RetObject := Pointer(SRPI_LuaToParaPkg(SRP_CLEInterface,Index));
        TVarData(Result).VDispatch := Pointer(IDispatch(TSRPVariant.Create(RetObject,CLEVAR_PARAPKG,false)));
    end;
  VSLUATYPE_BINBUF :
    begin
        TVarData(Result).vType := varDispatch;
        RetObject := Pointer(SRPI_LuaToBinBuf(SRP_CLEInterface,Index));
        TVarData(Result).VDispatch := Pointer(IDispatch(TSRPVariant.Create(RetObject,CLEVAR_BINBUF,false)));
    end;
  VSLUATYPE_SXML :
    begin
        TVarData(Result).vType := varDispatch;
        RetObject := Pointer(SRPI_LuaToSXml(SRP_CLEInterface,Index));
        TVarData(Result).VDispatch := Pointer(IDispatch(TSRPVariant.Create(RetObject,CLEVAR_SXML,false)));
    end;
  VSLUATYPE_FUNCTIONPARA :
    begin
        TVarData(Result).vType := varDispatch;
        RetObject := Pointer(SRPI_LuaToFunctionPara(SRP_CLEInterface,Index));
        TVarData(Result).VDispatch := Pointer(IDispatch(TSRPVariant.Create(RetObject,CLEVAR_FUNCTIONPARA,false)));
    end;
  VSLUATYPE_COMMINTERFACE :
    begin
        TVarData(Result).vType := varDispatch;
        RetObject := Pointer(SRPI_LuaToCommInterface(SRP_CLEInterface,Index));
        TVarData(Result).VDispatch := Pointer(IDispatch(TSRPVariant.Create(RetObject,CLEVAR_COMMINTERFACE,false)));
    end;
  VSLUATYPE_NIL :
    begin
        TVarData(Result).vType := varNull;
        TVarData(Result).VPointer := NULL;
    end;
  else
    RetResult := false;
    TVarData(Result).vType := varEmpty;
    TVarData(Result).VPointer := NULL;
  end;
end;

(*
  1. assign clevariant to variant array, reference will be increased auto.
  2. assign clevariant to pointer, reference will be not increased auto.

*)

function TSRPVariant.RetLuaResult(var VarResult;Number:Integer):Boolean;
var
  i : Integer;
  RetResult_Bool : Boolean;
  ResultArray : variant;
  tv : TVarData;
begin
  if PVarData(VarResult) = NULL then
  begin
    Result := true;
    exit;
  end;
  if Number = 1 then
  begin
    (*-- only one result --*)
    tv := TVarData(GetLuaValueAtIndex(-1,RetResult_Bool));
    if tv.VType = varDispatch then
      IDispatch(tv.VDispatch)._AddRef();
    PVarData(VarResult)^ := tv;
    Result := RetResult_Bool;
  end
  else
  begin
    (*--multiple parameter--*)
    ResultArray := VarArrayCreate([1,Number], varVariant);
    for i := 0 to Number - 1 do
    begin
      ResultArray[i+1] := GetLuaValueAtIndex(i - Number,RetResult_Bool);
      if RetResult_Bool = false then
      begin
        Result := RetResult_Bool;
        exit;
      end;
    end;
    PVarData(VarResult)^.VType := varArray;
    PVarData(VarResult)^.VArray := VarArrayAsPSafeArray(ResultArray);
    Result := true;
  end;
end;

