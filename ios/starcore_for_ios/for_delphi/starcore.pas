Constructor TSRPVariant.Create(In_CLEObject:Pointer;NeedRelease:Boolean);
begin
  if SRP_CLEInterface = NULL then
    MessageBox(HWND(NULL),'SRP_CLEInterface should be set before using TSRPVariant','Error',MB_OK);
  ObjectType := CLEVAR_OBJECT;
  CLEObject := In_CLEObject;
  NameNodeRoot := NULL;
  FRefCount := 1;
  if NeedRelease = false then
    SRPI_AddRefEx(SRP_CLEInterface,CLEObject);
end;

Constructor TSRPVariant.Create(In_CLEObject:Pointer; OT:Integer;NeedRelease:Boolean);
begin
  if SRP_CLEInterface = NULL then
    MessageBox(HWND(NULL),'SRP_CLEInterface should be set before using TSRPVariant','Error',MB_OK);
  ObjectType := OT;
  CLEObject := In_CLEObject;
  NameNodeRoot := NULL;
  FRefCount := 1;
  if NeedRelease = false then
    case OT of
    CLEVAR_OBJECT : SRPI_AddRefEx(SRP_CLEInterface,CLEObject);
    CLEVAR_PARAPKG : SRPParaPkg_AddRef(CLEObject);
    CLEVAR_BINBUF  : SRPBinBuf_AddRef(CLEObject);
    CLEVAR_SXML    : SRPSXml_AddRef(CLEObject);
    CLEVAR_FUNCTIONPARA    : SRPFP_AddRef(CLEObject);
    CLEVAR_COMMINTERFACE    : SRPComm_AddRef(CLEObject);
    end;
end;

Destructor TSRPVariant.Destroy;
var
  tp : PFuncOrVarNameNode;
begin
  if (CLEObject <> NULL) then
  begin
    case ObjectType of
    CLEVAR_OBJECT : SRPI_DelRefEx(SRP_CLEInterface,CLEObject);
    CLEVAR_PARAPKG : SRPParaPkg_Release(CLEObject);
    CLEVAR_BINBUF  : SRPBinBuf_Release(CLEObject);
    CLEVAR_SXML    : SRPSXml_Release(CLEObject);
    CLEVAR_FUNCTIONPARA    : SRPFP_Release(CLEObject);
    CLEVAR_COMMINTERFACE    : SRPComm_Release(CLEObject);
    end;
  end;
  CLEObject := NULL;
  while NameNodeRoot <> NULL do
  begin
    tp:= NameNodeRoot;
    NameNodeRoot := NameNodeRoot.Next;
    //New(FuncOrVarNameNode);
    Dispose(tp);
  end;
end;

