    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "(gdb) Attach",
            "type": "cppdbg",
            "request": "launch",
            "cwd": ".",
            /*"program": "/home/srplab/Desktop/flutter.study/starflut/example/build/linux/debug/bundle/starflut_example",*/
            "program": "C:/Users/srpla/Desktop/flutter.study/starflut/example/build/windows/runner/Debug/starflut_example.exe",
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "starflut",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "example",
            "cwd": "example",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug"
        }
    ]
}